{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.Mvc": "Information"
    }
  },
  "AllowedHosts": "*",
  "UserManagement": {
    "Provider": "postgresql",
    "ConnectionString": "Host=localhost;Database=userdb;Username=postgres;Password=postgres;Port=5433",
    "JwtSecret": "YourSuperSecretKeyForJWTTokenGeneration12345678901234567890",
    "ApplicationName": "DainnUserManagement",
    "Enable2FA": true,
    "AutoMigrateDatabase": true,
    "SeedDefaultAdmin": false,
    "DefaultAdminEmail": "admin@example.com",
    "DefaultAdminPassword": "Admin@123!",
    "EnforceHttps": false,
    "EnableHsts": false,
    "JwtSettings": {
      "AccessTokenHours": 1,
      "RefreshTokenDays": 7
    },
    "JwtValidation": {
      "ValidateIssuer": false,
      "ValidIssuer": null,
      "ValidateAudience": false,
      "ValidAudience": null,
      "ValidateLifetime": true,
      "ValidateIssuerSigningKey": true,
      "ClockSkewString": "00:00:00"
    },
    "SecuritySettings": {
      "MaxFailedLoginAttempts": 5,
      "LockoutDurationMinutes": 30,
      "EnableRateLimiting": true,
      "RateLimitRequestsPerMinute": 10
    },
    "CorsSettings": {
      "Enabled": false,
      "AllowedOrigins": [],
      "AllowedMethods": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
      "AllowedHeaders": [ "Content-Type", "Authorization", "X-Requested-With" ],
      "AllowCredentials": true
    },
    "OAuth2Settings": {
      "Google": {
        "Enabled": false,
        "ClientId": "YOUR_GOOGLE_CLIENT_ID",
        "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET"
      },
      "Microsoft": {
        "Enabled": false,
        "ClientId": "YOUR_MICROSOFT_APPLICATION_ID",
        "ClientSecret": "YOUR_MICROSOFT_CLIENT_SECRET"
      },
      "Facebook": {
        "Enabled": false,
        "AppId": "YOUR_FACEBOOK_APP_ID",
        "AppSecret": "YOUR_FACEBOOK_APP_SECRET"
      },
      "GitHub": {
        "Enabled": false,
        "ClientId": "YOUR_GITHUB_CLIENT_ID",
        "ClientSecret": "YOUR_GITHUB_CLIENT_SECRET"
      },
      "Apple": {
        "Enabled": false,
        "ClientId": "YOUR_APPLE_SERVICE_ID",
        "KeyId": "YOUR_APPLE_KEY_ID",
        "TeamId": "YOUR_APPLE_TEAM_ID",
        "PrivateKeyPath": "path/to/AuthKey_XXXXXXXXXX.p8"
      }
    }
  },
  "OpenTelemetry": {
    "Enabled": true,
    "OtlpTracesEndpoint": "http://localhost:4317",
    "ServiceName": "DainnUserManagement",
    "ServiceVersion": "2.0.0",
    "EnableAspNetCoreInstrumentation": true,
    "EnableHttpClientInstrumentation": true,
    "EnableEntityFrameworkCoreInstrumentation": true,
    "EnablePrometheusExporter": true
  },
  "Serilog": {
    "Enabled": true,
    "OtlpLogsEndpoint": "http://localhost:4318/v1/logs",
    "ServiceName": "DainnUserManagement",
    "ServiceVersion": "2.0.0",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithSpan"
    ]
  },
  "Swagger": {
    "Title": "DainnUserManagement API",
    "Description": "A comprehensive user management API with authentication, authorization, and admin features.",
    "ContactName": "UserManagement Support",
    "ContactEmail": "support@example.com",
    "RoutePrefix": "swagger",
    "DocumentTitle": "DainnUserManagement API Documentation"
  },
  "ApiVersioning": {
    "DefaultVersion": "1.0",
    "AssumeDefaultVersionWhenUnspecified": true,
    "ReportApiVersions": true,
    "EnableQueryStringReader": true,
    "QueryStringParameterName": "version",
    "EnableHeaderReader": true,
    "HeaderName": "X-Version",
    "EnableUrlSegmentReader": true,
    "GroupNameFormat": "'v'VVV",
    "SubstituteApiVersionInUrl": true
  }
}
